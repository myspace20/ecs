{
  "family": "java-web-taskdef",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::396608771737:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "java-web-app",
      "image": "396608771737.dkr.ecr.eu-central-1.amazonaws.com/java-web-app:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
