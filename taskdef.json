{
  "family": "java-web-service",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::396608771737:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "java-web-app",
      "image": "#{ImageURI}",
      "portMappings": [
        { "containerPort": 8080, "protocol": "tcp" }
      ],
      "essential": true
    }
  ]
}
